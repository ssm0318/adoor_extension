

{
  "name": "adoor(어도어) Notification Checker",
  "description": "Help users keep track of notifications from https://adoor.app .",
  "version": "1.2",
  "manifest_version": 2,
  "permissions": ["alarms", "notifications", "storage", "tabs", "webNavigation", "*://adoor.app/*"],
  "background": {
    "persistent": false,
    "page": "background.html"
  },
  "browser_action": {
    "default_title": "Adoor Notification",
    "default_icon": "icon19.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "adoor128.png"
  }
}
